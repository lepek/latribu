<% if current_user.is_new? -%>
    <% content_for :stylesheets do %>
        <%= stylesheet_link_tag :datatables %>
    <% end %>

    <% content_for :javascripts do %>
        <%= javascript_include_tag :datatables, :clients_index %>
    <% end %>

    <!-- The master panel with admin grid -->
    <div id="content-inner">
      <h1>Autogesti&oacute;n</h1>
      <div id="tabs" style="width: 100%">
        <ul>
          <% if can? :manage, Inscription %><li><a href="#inscriptions">Pr&oacute;ximas clases</a></li><% end %>
          <% if can? :manage, Inscription %><li><a href="#events">Mis clases</a></li><% end %>
        </ul>

        <% if can? :manage, Inscription -%>
            <div id="inscriptions">
              <%= render "inscriptions/tab", :shifts => @shifts %>
            </div>
        <% end -%>

        <% if can? :manage, Inscription -%>
            <div id="events">
              <%= render "my_classes/tab", :events => @events %>
            </div>
        <% end -%>


      </div>
    </div>
<% end -%>