<!DOCTYPE html>
<html>
<head>
  <title>Nahual</title>
  <%= stylesheet_link_tag media: "all", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag :default, :session %>
  <!--[if lte IE 6]><%= stylesheet_link_tag :ie6 %><![endif]-->
  <%= yield :stylesheets %>
  <%= javascript_include_tag "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>

<body>
<div id="content-outer" class="session">
  <%= render :partial => 'layouts/topbar' %>
  <section id="content" class="session">
    <%= yield %>
  </section>
</div>
</body>